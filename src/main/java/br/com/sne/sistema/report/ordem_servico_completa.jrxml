		<field name="quantidade" class="java.lang.Integer"/>
		<field name="totalEmpresa" class="java.lang.Float"/>
		<field name="totalFornecedor" class="java.lang.Float"/>
		<field name="calcularD" class="java.lang.Boolean"/>
