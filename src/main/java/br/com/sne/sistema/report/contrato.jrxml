	<queryString>
		<![CDATA[SELECT
	c.cnpj as cnpj_cliente,c.nome as nome_cliente,
	end_cli.logradouro as logradouro_cliente,
	end_cli.bairro as bairro_cliente, end_cli.cidade as cidade_cliente,
	end_cli.estado as estado_cliente, end_cli.numero as casa_cliente,
	end_cli.cep as cep_cliente, o.datainicio as datainicioos,
	o.datafim as datafimos, o.horainicio as horainicio,
	o.horafim as horafim, le.nome as nome_le,
	o.nomeEvento as nome_evento,o.preco as preco_evento,
	(preco - o.desconto) as preco_final, o.id as idos,
    amb.id as id_ambiente, o.detalhesevento, o.desconto,
	o.terceirizadoempresa as terEmp, o.subtotalTerceirizadoForn as tFornecedor,
    totalll_logistica.t as total_logistica

FROM
	ordemservico o
		left join funcionario f on (f.id = o.funcionario)
		left join cliente c on (c.id = o.cliente)
		left join localevento le on (le.id = o.local)
		left join endereco end_cli on ( end_cli.id = c.endereco)
		left join endereco end_le on ( end_le.id = le.endereco)
		left join ambienteevento amb on amb.idordemservico = o.id,
        (SELECT sum(subtotal) as t from recursosolicitado,ambienteevento as amb
			WHERE amb.nome = 'LOGÍSTICA' and amb.id = recursosolicitado.ambiente and recursosolicitado.idordemservico = $P{id}
		) as totalll_logistica

WHERE
	o.id = $P{id}
ORDER BY
	id_ambiente]]>
	</queryString>
