	<subDataset name="Table Dataset 1" uuid="e6b77c9e-90ce-4482-ada5-6614a7671a31">
		<queryString>
			<![CDATA[SELECT
     recurso.`codigo` AS recurso_codigo,
     recurso.`descricao` AS recurso_descricao,
     CASE equipamento.`status`
       	WHEN 0 THEN 'DISPONÍVEIS'
       	WHEN 1 THEN 'LOCADOS'
	WHEN 2 THEN 'ASSISTÊNCIA TÉC.'
	WHEN 3 THEN 'PENDENTES'
	WHEN 4 THEN 'DESCARTADOS'
     END AS status,
     COUNT(equipamento.`status`) AS qtdStatus,
     recurso.`nome` AS recurso_nome,
     grupo.`nome` AS grupo_nome,
     grupo.`codigo` AS grupo_codigo

FROM
     `grupo` grupo LEFT JOIN `recurso` recurso ON grupo.`id` = recurso.`grupo`
     LEFT JOIN `equipamento` equipamento ON grupo.`id` = equipamento.`grupo`
GROUP BY grupo.`id`, equipamento.`status`]]>
		</queryString>
		<field name="recurso_codigo" class="java.lang.String"/>
		<field name="recurso_descricao" class="java.lang.String"/>
		<field name="status" class="java.lang.String"/>
		<field name="qtdStatus" class="java.lang.Long"/>
		<field name="recurso_nome" class="java.lang.String"/>
		<field name="grupo_nome" class="java.lang.String"/>
		<field name="grupo_codigo" class="java.lang.String"/>
		<group name="group1"/>
	</subDataset>
	<parameter name="imagem" class="java.lang.Object"/>
	<queryString>
		<![CDATA[select  r.nome as recurso, g.nome as grupo, count(e.id) as qtd, r.descricao,
    CASE e.status
	WHEN 0 THEN 'DISPONÍVEIS'
       	WHEN 1 THEN 'LOCADOS'
	WHEN 2 THEN 'ASSISTÊNCIA TÉC.'
	WHEN 3 THEN 'PENDENTES'
	WHEN 4 THEN 'DESCARTADOS'
     END AS statuss
from equipamento e inner join grupo g
	on e.grupo = g.id
	inner join recurso r
	on r.grupo = g.id
	left join equipamentosublocado s
	on s.equipamento_id = e.id
where e.descricaoequipamento = r.id
	and s.equipamento_id is null
group by g.nome, r.nome, statuss, r.descricao
order by g.nome, r.nome]]>
	</queryString>
