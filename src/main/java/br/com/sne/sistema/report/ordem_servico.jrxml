	<subDataset name="RecursosSolicitados" uuid="c9d6af99-fe37-4f44-9ab6-a4896a19249e">
		<parameter name="id_orcamento" class="java.lang.Long"/>
		<parameter name="id_ambiente" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT
    	recursosolicitado.descricao as nome, recursosolicitado.precoUnitario as precounitario, quantidade, diarias, subtotal, totalll.t as total, grupo, grupo.nome as nome_grupo,
	grupo.tiporecurso as tipoRecurso,
	amb.nome as amb_nome, amb.dataInicio as amb_dti, amb.dataFim as amb_dtf
FROM
	recursosolicitado, grupo, recurso, ambienteevento as amb ,
		(SELECT sum(subtotal) as t from recursosolicitado
			WHERE idordemservico = $P{id_orcamento} and ambiente = $P{id_ambiente}
		) as totalll
WHERE
	recursosolicitado.recurso = recurso.id and recursosolicitado.idordemservico = $P{id_orcamento}
	and ambiente = $P{id_ambiente} and recursosolicitado.ambiente = amb.id
        and recurso.grupo = grupo.id
ORDER BY grupo.tiporecurso asc, grupo.id asc,recursosolicitado.id asc]]>
		</queryString>
		<field name="nome" class="java.lang.String"/>
		<field name="precounitario" class="java.math.BigDecimal"/>
		<field name="quantidade" class="java.lang.Integer"/>
		<field name="diarias" class="java.math.BigDecimal"/>
		<field name="subtotal" class="java.math.BigDecimal"/>
		<field name="total" class="java.math.BigDecimal"/>
		<field name="amb_nome" class="java.lang.String"/>
		<field name="amb_dti" class="java.util.Date"/>
		<field name="amb_dtf" class="java.util.Date"/>
		<field name="grupo" class="java.lang.Integer"/>
		<field name="nome_grupo" class="java.lang.String"/>
		<field name="tipoRecurso" class="java.lang.Long"/>
		<variable name="total" class="java.math.BigDecimal" resetType="None" calculation="Sum">
			<variableExpression><![CDATA[$F{diarias}]]></variableExpression>
			<initialValueExpression><![CDATA[java.math.BigDecimal.valueOf(0)]]></initialValueExpression>
		</variable>
		<group name="group1">
			<groupExpression><![CDATA[$F{grupo}]]></groupExpression>
		</group>
	</subDataset>
	<subDataset name="RecursosTerceirizados" uuid="0c4a8773-fec7-4e43-b557-a63e4142dedb">
		<parameter name="idos" class="java.lang.Long"/>
		<parameter name="id_ambiente" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT
    	recursoterceirizadosolicitado.descricao as nome,recursoterceirizadosolicitado.quantidade as quantidade, recursoterceirizadosolicitado.precoUnitario as precoTercei, recursoterceirizado.precoFornecedor as precoFornecedor, diarias, (recursoterceirizado.precoEmpresa * quantidade) as subtotal, totalll.t as total, totalFornecedor.tf as tForn, grupo, grupo.nome as nome_grupo, amb.nome as amb_nome, amb.dataInicio as amb_dti, amb.dataFim as amb_dtf,(recursoterceirizado.precoFornecedor * quantidade) as sForn
FROM
	recursoterceirizadosolicitado, grupo, recursoterceirizado, ambienteevento as amb ,
		(SELECT sum(precoEmpresa * quantidade) as t from recursoterceirizadosolicitado
			WHERE idordemservico = $P{idos} and ambiente = $P{id_ambiente}
		) as totalll ,
		(SELECT sum(precoFornecedor * quantidade) as tf from recursoterceirizadosolicitado
			WHERE idordemservico = $P{idos} and ambiente = $P{id_ambiente}
		) as totalFornecedor
WHERE
	recursoterceirizadosolicitado.recurso = recursoterceirizado.id and recursoterceirizadosolicitado.idordemservico = $P{idos}
	and ambiente = $P{id_ambiente} and recursoterceirizadosolicitado.ambiente = amb.id
        and recursoterceirizado.grupo = grupo.id
ORDER BY grupo.id asc,recursoterceirizadosolicitado.id asc]]>
		</queryString>
		<field name="nome" class="java.lang.String"/>
		<field name="precotercei" class="java.math.BigDecimal"/>
		<field name="precofornecedor" class="java.math.BigDecimal"/>
		<field name="diarias" class="java.math.BigDecimal"/>
		<field name="subtotal" class="java.math.BigDecimal"/>
