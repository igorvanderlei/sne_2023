	<queryString>
		<![CDATA[SELECT
	o.terceirizadoempresa as terEmp, o.id as idOS, o.dataInicio as dataInicioOS, o.dataFim as dataFimOS, o.nomeEvento,  o.observacoes
	as observacoesOS, f.id as id_funcionario, f.nome as nome_funcionario, c.id as id_cliente,  c.nome
	as nome_cliente,le.nome , end_le.logradouro as local_logradouro, end_le.numero as local_numero,
	end_le.complemento as local_complemento, end_le.bairro as local_bairro, end_le.cidade as local_cidade,
	end_le.cep as local_cep, end_le.estado as local_estado, end_le.pontoreferencia as
	loca_ponto_referencia, o.datamontagem, o.horainicio, o.horafim, o.horamontagem, c.fone, c.email,
	o.observacoesCliente as observacoes, o.condicoesPagamento as condicoes, f.celular as
	celular_funcionario, amb.nomeambiente as nome_ambiente, amb.idambiente as id_ambiente,
	o.desconto as desconto, o.preco as preco, (preco - desconto) as preco_final, o.nomeProposta
	as nome_proposta, o.telefoneproposta as telefone_proposta, o.cargoproposta as cargo_proposta,
	o.contatoEvento as nome_contato, o.telefoneContatoEvento as telefone_contato,
	le.observacoes as observacoes_local, o.detalhesevento
FROM
	ordemservico o
		left join funcionario f on (f.id = o.funcionario)
		left join cliente c on (c.id = o.cliente)
		left join localevento le on (le.id = o.local)
		left join endereco end_le on ( end_le.id = le.endereco)
		inner join (
			SELECT
				ambienteevento.id as idambiente, ambienteevento.nome as nomeambiente ,
				count(recursosolicitado.id) as qtd, ambienteevento.idordemservico as idos
			FROM
				ambienteevento
				INNER JOIN recursosolicitado ON recursosolicitado.ambiente = ambienteevento.id
			WHERE
				ambienteevento.idordemservico is not null
			GROUP BY
				ambienteevento.id, ambienteevento.nome, ambienteevento.idordemservico
		) amb on amb.idos = o.id
WHERE
	o.id = $P{id}
GROUP BY
	nome_ambiente, o.id, o.dataInicio, o.dataFim, o.nomeEvento,  o.observacoes, f.id, f.nome, c.id,
	c.nome, le.nome , end_le.logradouro, end_le.numero, end_le.complemento, end_le.bairro, end_le.cidade,
	end_le.cep , end_le.estado , end_le.pontoreferencia, o.datamontagem, o.horainicio, o.horafim,
	o.horamontagem, c.fone, c.email, o.observacoesCliente , o.condicoesPagamento, f.celular,
	amb.idambiente, o.desconto, o.preco, preco_final, o.nomeProposta, o.telefoneproposta,
	o.cargoproposta, o.contatoEvento, o.telefoneContatoEvento, le.observacoes,  o.detalhesevento
ORDER BY
	amb.idambiente]]>
	</queryString>
