	<queryString>
		<![CDATA[SELECT codigo AS idrec,
	codigo AS idgrp,
	nome AS grupo,
	nome AS recurso,
	total AS disponiveis,
	total AS locados,
	total AS manutPrev,
	total AS manutCorre,
	total AS descartados
FROM grupo g
	inner join recurso r ON 
	 'NULL' = 'NULL' 
	left join (SELECT descricaoequipamento AS id_subgrupo,
	count( descricaoequipamento) AS total
FROM equipamento
	left join equipamentosublocado s ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' IS NULL  
	 AND equipamento.status = {1} 
	 )  AS subDisp ON 'NULL' = 'NULL' 
	left join (SELECT descricaoequipamento AS id_subgrupo,
	count( descricaoequipamento) AS total
FROM equipamento
	left join equipamentosublocado s ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' IS NULL  
	 AND equipamento.status = 1 
	 )  AS subLoc ON 'NULL' = 'NULL' 
	left join (SELECT descricaoequipamento AS id_subgrupo,
	count( descricaoequipamento) AS total
FROM equipamento
	left join equipamentosublocado s ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' IS NULL  
	 AND equipamento.status = 2 
	 )  AS subManut ON 'NULL' = 'NULL' 
	left join (SELECT descricaoequipamento AS id_subgrupo,
	count( descricaoequipamento) AS total
FROM equipamento
	left join equipamentosublocado s ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' IS NULL  
	 AND equipamento.status = 3 
	 )  AS subManutC ON 'NULL' = 'NULL' 
	left join (SELECT descricaoequipamento AS id_subgrupo,
	count( descricaoequipamento) AS total
FROM equipamento
	left join equipamentosublocado s ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' IS NULL  
	 AND equipamento.status = 4 
	 )  AS subDesc ON 'NULL' = 'NULL' 
	inner join (SELECT descricaoequipamento AS id_subgrupo,
	count( descricaoequipamento) AS total
FROM equipamento
	left join equipamentosublocado s ON 
	 'NULL' = 'NULL' 
WHERE 
	 'NULL' IS NULL  
	 )  AS subTotalGeral ON 'NULL' = 'NULL' 
ORDER BY idgrp DESC,
	nome DESC]]>
	</queryString>
